<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#6000FF00" />
            <stroke android:color="#40FFFF00" android:dashWidth="6dip" android:dashGap="6dip" android:width="1dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#200000FF" />
            <stroke android:color="#40FFFF00" android:dashWidth="6dip" android:dashGap="6dip" android:width="1dip" />
        </shape>
    </item>
</selector>